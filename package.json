{
  "name": "securevault",
  "version": "1.0.0",
  "description": "A secure web-based password manager with client-side AES encryption",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "test:blockchain": "node test-sepolia.js",
    "deploy:contract": "node scripts/deploy-sepolia.js"
  },
  "keywords": [
    "password-manager",
    "encryption",
    "security",
    "aes",
    "react",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "node-fetch": "^2.7.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "stripe": "^18.4.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
